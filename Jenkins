pipeline {
    agent {
        dockerfile true
    }
    stages{
        stage('Run App') {
            steps {
                sh 'python3 ./app.py'
        }
    }
        stage('Test with E2E') {
            steps {
                    output = sh 'python3 ./tests/E2E.py'
                    echo "the result is ${output}"
            }
        }
    }
}
